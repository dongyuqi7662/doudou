<script>
	// 主体左侧部分添加背景图
	for(var i=0;i<$(".collapse").length;i++){
		var j=i+1;
		var ocss="#575757 url(/images/menu_"+j+".png) no-repeat";
		$(".collapse").eq(i).css("background",ocss)

	}
	// 二级菜单
	$(".collapse").click(function(){
		var index=$(this).index();
		var omenv=$(".menv").eq(index).css("display");
		if(omenv=="none"){
			$(".menv").eq(index).css("display","block");
			var i=index+1;
			var bcss="#575757 url(/images/menu1_"+i+".png) no-repeat";
			$(this).css("background",bcss)
		}else{
			$(".menv").eq(index).css("display","none");
			var i=index+1;
			var ocss="#575757 url(/images/menu_"+i+".png) no-repeat";
			$(this).css("background",ocss)
		}
		
	})
	$(".collapse").mouseover(function(){
		$(this).css("background-color","#303030")
	}).mouseout(function(){
		$(this).css("background-color","#575757")
	})
	
</script>